<keymap version="1" name="Default for Windows copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
    <mouse-shortcut keystroke="control button3 doubleClick" />
  </action>
  <action id="CloseAllEditors">
    <keyboard-shortcut first-keystroke="ctrl alt c" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="ctrl comma" />
  </action>
  <action id="EslintImportCodeStyle">
    <keyboard-shortcut first-keystroke="ctrl alt semicolon" />
  </action>
  <action id="IntroduceConstant" />
  <action id="IntroduceField" />
  <action id="Javascript.Linters.EsLint.Fix">
    <keyboard-shortcut first-keystroke="ctrl alt quote" />
  </action>
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="ctrl alt 3" />
  </action>
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="ctrl alt 2" />
  </action>
  <action id="ShowFilterPopup" />
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift ctrl x" />
  </action>
  <action id="TodoViewGroupByFlattenPackage" />
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="ctrl alt d" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="ctrl alt f" />
  </action>
</keymap>