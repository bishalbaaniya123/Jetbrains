<keymap version="1" name="Default copy" parent="Default for XWin">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <mouse-shortcut keystroke="control button3 doubleClick" />
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
  </action>
  <action id="CloseEditor">
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorDecreaseFontSize">
    <mouse-shortcut keystroke="control button142" />
  </action>
  <action id="EditorIncreaseFontSize">
    <mouse-shortcut keystroke="control button143" />
  </action>
  <action id="EditorPasteFromX11" />
  <action id="EditorSelectWord" />
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
  </action>
  <action id="RunClass">
    <keyboard-shortcut first-keystroke="shift ctrl f10" />
    <mouse-shortcut keystroke="alt button3 doubleClick" />
    <keyboard-shortcut first-keystroke="ctrl alt semicolon" />
  </action>
</keymap>